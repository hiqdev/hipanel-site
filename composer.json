{
    "name": "hiqdev/hipanel-site",
    "type": "yii2-extension",
    "description": "Selling site for HiPanel",
    "keywords": [
        "hipanel",
        "site"
    ],
    "homepage": "http://hipanel.com/",
    "license": "BSD-3-Clause",
    "support": {
        "email": "support@hiqdev.com",
        "source": "https://github.com/hiqdev/provider-site",
        "issues": "https://github.com/hiqdev/provider-site/issues",
        "wiki": "https://github.com/hiqdev/provider-site/wiki",
        "forum": "http://forum.hiqdev.com/"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "role": "Project lead",
            "email": "sol@hiqdev.com",
            "homepage": "http://hipanel.com/"
        },
        {
            "name": "Dmitry Naumenko",
            "role": "Lead backend developer",
            "email": "d.naumenko.a@gmail.com",
            "homepage": "http://silverfire.me/"
        },
        {
            "name": "Andrey Klochok",
            "role": "Lead frontend developer",
            "email": "andreyklochok@gmail.com",
            "homepage": "http://hiqdev.com/"
        },
        {
            "name": "Yuriy Myronchuk",
            "role": "QA Lead",
            "email": "bladeroot@gmail.com",
            "homepage": "http://hiqdev.com/"
        }
    ],
    "require": {
        "codemix/yii2-localeurls": "^1.4",
        "creocoder/yii2-flysystem": "^0.8.1",
        "hiqdev/hisite": "<2.0 || dev-master",
        "hiqdev/hisite-module-news": "<2.0 || dev-master",
        "hiqdev/yii2-hiam-authclient": "<2.0 || dev-master",
        "hiqdev/yii2-cart": "<2.0 || dev-master",
        "hiqdev/yii2-language": "<2.0 || dev-master",
        "hiqdev/yii2-theme-flat": "<2.0 || dev-master",
        "hiqdev/yii2-theme-dataserv": "<2.0 || dev-master",
        "hiqdev/yii2-theme-original": "<2.0 || dev-master",
        "hiqdev/yii2-theme-sailor": "<2.0 || dev-master",
        "hiqdev/yii2-module-pages": "<2.0 || dev-master",
        "hiqdev/yii2-cookie-consent": "<2.0 || dev-master",
        "hiqdev/hipanel-module-domain": "<2.0 || dev-master",
        "hiqdev/hipanel-domain-checker": "<2.0 || dev-master",
        "hiqdev/hipanel-module-dns": "<2.0 || dev-master",
        "hiqdev/hipanel-module-stock": "<2.0 || dev-master",
        "hiqdev/hipanel-module-finance": "<2.0 || dev-master",
        "hiqdev/hipanel-module-server": "<2.0 || dev-master",
        "hiqdev/yii2-asset-icheck": "~1.0.2.5",
        "hiqdev/hipanel-faq": "<2.0 || dev-master"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0 || dev-master",
        "hiqdev/hidev-hiqdev": "<2.0 || dev-master"
    },
    "autoload": {
        "psr-4": {
            "hipanel\\site\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "params": "config/params.php",
            "common": "config/common.php",
            "web": "config/web.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
